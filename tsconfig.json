{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [
        "esnext.asynciterable",
        "es2015",
        "dom"
      ],
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true,
      "jsx": "react",
    },
    "exclude": [
      "./node_modules/"
    ]
}
